{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    ":separateMajorReleases",
    ":ignoreUnstable",
    ":ignoreModulesAndTests",
    ":automergeDisabled"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["com.google.errorprone:error_prone_core"],
      "matchUpdateTypes": ["major", "minor"],
      "enabled": false
    }
  ]
}
