<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>gcs-analytics-core-parent</artifactId>
    <groupId>com.google.cloud.gcs</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>gcs-analytics-core</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.6.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <shadedArtifactAttached>true</shadedArtifactAttached>
              <shadedClassifierName>shaded</shadedClassifierName>
              <minimizeJar>true</minimizeJar>
              <artifactSet>
                <excludes>
                  <exclude>org.conscrypt:conscrypt-openjdk-uber</exclude>
                  <exclude>io.grpc:grpc-netty-shaded</exclude>
                  <exclude>com.google.cloud:google-cloud-storage</exclude>
                </excludes>
              </artifactSet>
              <relocations>
                <relocation>
                  <pattern>com.google.auto.value</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.auto.value</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.common</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.common</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.apache.commons.lang3</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.org.apache.commons.lang3</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.api</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.api</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.auth</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.auth</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.cloud.storage</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.cloud.storage</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.cloud.http</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.cloud.http</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.cloud.iam</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.cloud.iam</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.cloud.monitoring</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.cloud.monitoring</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.cloud.opentelemetry</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.cloud.opentelemetry</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.gson</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.gson</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.iam</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.iam</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.longrunning</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.longrunning</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.protobuf</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.protobuf</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.rpc</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.rpc</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.google.type</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.com.google.type</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>io.grpc</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.io.grpc</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>io.opencensus</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.io.opencensus</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>io.opentelemetry</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.io.opentelemetry</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.api</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.api</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.cloud.storage</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.cloud.storage</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.iam</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.iam</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.longrunning</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.longrunning</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.protobuf</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.protobuf</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.rpc</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.rpc</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>google.type</pattern>
                  <shadedPattern>com.google.cloud.gcs.analyticscore.shaded.google.type</shadedPattern>
                </relocation>
              </relocations>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                    <exclude>**/module-info.class</exclude>
                    <exclude>**/package-info.class</exclude>
                    <exclude>**/*.md</exclude>
                    <exclude>META-INF/LICENSE*</exclude>
                    <exclude>META-INF/NOTICE*</exclude>
                    <exclude>META-INF/README*</exclude>
                    <exclude>**/*.html</exclude>
                  </excludes>
                </filter>
                <filter>
                  <artifact>com.google.cloud.gcs:core</artifact>
                  <includes>
                    <include>com/google/cloud/gcs/analyticscore/core/**</include>
                  </includes>
                </filter>
                <filter>
                  <artifact>com.google.cloud.gcs:client</artifact>
                  <includes>
                    <include>com/google/cloud/gcs/analyticscore/client/**</include>
                  </includes>
                </filter>
              </filters>
              <transformers>
                <transformer />
                <transformer />
                <transformer />
                <transformer>
                  <mainClass />
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>com.google.code.findbugs</groupId>
      <artifactId>jsr305</artifactId>
      <version>3.0.2</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.google.truth</groupId>
      <artifactId>truth</artifactId>
      <version>1.4.4</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>junit</artifactId>
          <groupId>junit</groupId>
        </exclusion>
        <exclusion>
          <artifactId>asm</artifactId>
          <groupId>org.ow2.asm</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <version>5.13.4</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>opentest4j</artifactId>
          <groupId>org.opentest4j</groupId>
        </exclusion>
        <exclusion>
          <artifactId>junit-platform-commons</artifactId>
          <groupId>org.junit.platform</groupId>
        </exclusion>
        <exclusion>
          <artifactId>apiguardian-api</artifactId>
          <groupId>org.apiguardian</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.mockito</groupId>
      <artifactId>mockito-junit-jupiter</artifactId>
      <version>5.19.0</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>mockito-core</artifactId>
          <groupId>org.mockito</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <maven.compiler.target>11</maven.compiler.target>
    <maven.compiler.source>11</maven.compiler.source>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
